{
	"schemaVersion": 1,
	"id": "agricraft",
	"version": "${version}",
	"name": "AgriCraft",
	"description": "Agricultural farming extended",
	"authors": [
		"InfinityRaider",
		"Ketheroth"
	],
	"contact": {
		"website": "https://www.curseforge.com/minecraft/mc-mods/agricraft",
		"repo": "https://github.com/AgriCraft/AgriCraft",
		"issues": "https://github.com/AgriCraft/AgriCraft/issues"
	},
	"license": "MIT",
	"icon": "logo.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.agricraft.agricraft.fabric.AgriCraftFabric"
		],
		"client": [
			"com.agricraft.agricraft.client.AgriCraftFabricClient"
		],
		"emi": [
			"com.agricraft.agricraft.compat.emi.AgriCraftEmiPlugin"
		],
		"modmenu": [
			"com.agricraft.agricraft.common.config.fabric.ModMenuConfig"
		],
		"jei_mod_plugin": [
			"com.agricraft.agricraft.compat.jei.AgriCraftJeiPlugin"
		],
		"jade": [
			"com.agricraft.agricraft.compat.AgriCraftJadePlugin"
		]
	},
	"mixins": [
		"agricraft-fabric.mixins.json"
	],
	"depends": {
		"fabric": "*",
		"minecraft": ">=1.20.4",
		"resourcefulconfig": ">=2.4.2"
